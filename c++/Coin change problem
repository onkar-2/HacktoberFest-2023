#include <bits/stdc++.h>
using namespace std;

// Recursive solution
int solve(vector<int> &num, int x) {
    if (x == 0) {
        return 0;
    }
    if (x < 0) {
        return INT_MAX;
    }
    int mini = INT_MAX;
    for (int i = 0; i < num.size(); i++) {
        int ans = solve(num, x - num[i]);
        if (ans != INT_MAX) {
            mini = min(mini, 1 + ans);
        }
    }
    return mini;
}

// Recursive + memoization solution
int solvemem(vector<int> &num, int x, vector<int> &dp) {
    if (x == 0) {
        return 0;
    }
    if (x < 0) {
        return INT_MAX;
    }
    if (dp[x] != -1) {
        return dp[x];
    }
    int mini = INT_MAX;
    for (int i = 0; i < num.size(); i++) {
        int ans = solvemem(num, x - num[i], dp);
        if (ans != INT_MAX) {
            mini = min(mini, 1 + ans);
        }
    }
    dp[x] = mini;
    return dp[x];
}

// Tabulation solution
int solvetab(vector<int> &num, int x) {
    vector<int> dp(x + 1, INT_MAX);
    dp[0] = 0;
    for (int i = 1; i <= x; i++) {
        for (int j = 0; j < num.size(); j++) {
            if (i - num[j] >= 0 && dp[i - num[j]] != INT_MAX) {
                dp[i] = min(dp[i], 1 + dp[i - num[j]]);
            }
        }
    }
    return (dp[x] == INT_MAX) ? -1 : dp[x];
}

// Function to return the minimum elements needed for a given sum
int minimumElements(vector<int> &num, int x, int approach = 1) {
    if (approach == 1) {
        // Recursive approach
        int ans = solve(num, x);
        return (ans == INT_MAX) ? -1 : ans;
    } else if (approach == 2) {
        // Recursive + memoization approach
        vector<int> dp(x + 1, -1);
        int ans = solvemem(num, x, dp);
        return (ans == INT_MAX) ? -1 : ans;
    } else {
        // Tabulation approach
        return solvetab(num, x);
    }
}

int main() {
    vector<int> num = {1, 2, 3}; // Example input vector
    int x = 5; // Example value of x

    // Test different approaches
    cout << "Minimum number of elements required to form " << x << " (Recursive): " << minimumElements(num, x, 1) << endl;
    cout << "Minimum number of elements required to form " << x << " (Memoization): " << minimumElements(num, x, 2) << endl;
    cout << "Minimum number of elements required to form " << x << " (Tabulation): " << minimumElements(num, x, 3) << endl;

    return 0;
}
